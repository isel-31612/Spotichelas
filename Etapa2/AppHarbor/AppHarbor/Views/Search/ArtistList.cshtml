@model IEnumerable<Utils.ViewArtist>

@{
    ViewBag.Title = "ArtistList";
}

<h2>ArtistList</h2>

<table>
    <tr>
        <th>
            Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Details", "Artist", new { href=item.Href })
        </td>
    </tr>
}

</table>
